{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)\\.html",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    },
    {
      "source": "/(.*)\\.(js|css|woff2|png|jpg|jpeg|gif|svg|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/rss.xml",
      "destination": "https://materialblog-server-production.up.railway.app/api/rss.xml"
    }
  ]
}
